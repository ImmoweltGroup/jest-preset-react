{
  "name": "@immowelt/jest-preset-react",
  "version": "0.0.0-development",
  "private": false,
  "author": "Immonet dev team <aegaeon@immonet.de> (https://immonet.de)",
  "license": "MIT",
  "description": "Common configuration options for Jest in React environments.",
  "main": "jest-preset.json",
  "scripts": {
    "test": "npm run lint && npm run jest:coverage",
    "lint": "eslint src",
    "jest": "jest",
    "jest:watch": "npm run jest -- --watch",
    "jest:coverage": "npm run jest -- --coverage",
    "semantic-release": "semantic-release pre && npm publish --access=public && semantic-release post"
  },
  "engines": {
    "node": ">=8.2.0",
    "npm": ">=5.3.0"
  },
  "devDependencies": {
    "@inkdpixels/commit-analyzer": "^1.0.1",
    "@inkdpixels/release-notes-generator": "^1.0.1",
    "@immowelt/eslint-config-immowelt-react": "^1.0.0",
    "@immowelt/jest-preset-node": "^1.0.0",
    "eslint": "4.4.1",
    "jest": "^20.0.4",
    "semantic-release": "^6.3.6"
  },
  "jest": {
    "preset": "@immowelt/jest-preset-node"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ImmoweltGroup/jest-preset-react.git"
  },
  "release": {
    "analyzeCommits": "@inkdpixels/commit-analyzer",
    "generateNotes": "@inkdpixels/release-notes-generator"
  }
}
